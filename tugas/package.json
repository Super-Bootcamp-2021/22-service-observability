{
  "name": "task-manager",
  "version": "1.0.1",
  "description": "sample service mesh implementation",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha",
    "svc:start": "node .",
    "svc:dev": "nodemon .",
    "lint": "eslint .",
    "web:build": "webpack --mode production",
    "web:dev": "webpack serve --open --mode development",
    "service": "ts-node --project service/tsconfig.json service/server.ts",
    "service:dev": "nodemon --config service/nodemon.json",
    "docs:svc": "typedoc --tsconfig service/tsconfig.json --options service/docs.json",
    "docs:web": "typedoc --tsconfig tsconfig.json --options docs.json",
    "svc:all": "node ./service/main.js worker & node ./service/main.js task & node ./service/main.js performance"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "css-loader": "^5.0.1",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-prettier": "^3.3.1",
    "json-loader": "^0.5.7",
    "livereload": "^0.9.1",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "yaml-loader": "^0.6.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "busboy": "^0.3.1",
    "mariadb": "^2.5.2",
    "mime-types": "^2.1.28",
    "minio": "^7.0.18",
    "mysql": "^2.18.1",
    "nats": "^1.4.12",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "rc": "^1.2.8",
    "redis": "^3.0.2",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.6.3",
    "swagger-ui": "^3.42.0",
    "typedoc": "^0.20.25",
    "typeorm": "^0.2.30",
    "vue": "^2.6.12"
  }
}
